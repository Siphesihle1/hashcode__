function sumto(arr, target)
{
	findex = 0
	sindex = 1
	sum = arr[psum]

	
	
	for (int i = findex; i < arr.length; i++)
	{
		for (int j = 1; j < arr.length; j++) 
		{
			for (int k = sindex; k < arr.length; k++)
			{
			
				if (sum < target)
				{
					sum += arr[j]
					numbers.push(arr[j])			
				}
				else if (sum > target)
				{
					sindex++
					numbers.clear()
					sum = 0
					break
				}
				else
				{
					return numbers
				}
				
			}
		}
	}
	return numbers
}
